{
  "name": "linkloud_server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "migration:create": "yarn typeorm migration:create src/migrations/file",
    "migration:generate": "ENV=production yarn typeorm migration:generate src/migrations/file -d src/configs/datasource.ts",
    "migration:run:dev": "ENV=development yarn typeorm migration:run -d src/configs/datasource.ts",
    "migration:revert:dev": "ENV=development yarn typeorm migration:revert -d src/configs/datasource.ts",
    "migration:run:prod": "ENV=production yarn typeorm migration:run -d src/configs/datasource.ts",
    "migration:revert:prod": "ENV=production yarn typeorm migration:revert -d src/configs/datasource.ts"
  },
  "dependencies": {
    "@nestjs/axios": "^2.0.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/schedule": "^3.0.1",
    "@nestjs/swagger": "^6.3.0",
    "@nestjs/typeorm": "^9.0.0-next.2",
    "@sendgrid/mail": "^7.7.0",
    "@sentry/node": "^7.53.1",
    "@slack/client": "^5.0.2",
    "@types/bcrypt": "^5.0.0",
    "@types/morgan": "^1.9.4",
    "@types/winston": "^2.4.4",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dayjs": "^1.11.7",
    "express-basic-auth": "^1.2.1",
    "fs": "^0.0.1-security",
    "joi": "^17.9.2",
    "metadata-scraper": "^0.2.61",
    "morgan": "^1.10.0",
    "mysql2": "^3.2.4",
    "nest-winston": "^1.9.2",
    "passport-jwt": "^4.0.1",
    "path": "^0.12.7",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "typeorm": "0.3.11",
    "typeorm-naming-strategies": "^4.1.0",
    "web-push": "^3.6.3",
    "winston": "^3.8.2",
    "winston-logzio": "^5.1.7"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/cron": "^2.0.1",
    "@types/express": "^4.17.13",
    "@types/jest": "29.2.4",
    "@types/node": "18.11.18",
    "@types/supertest": "^2.0.11",
    "@types/web-push": "^3.3.2",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.3.1",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.1.1",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
